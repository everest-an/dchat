# Dchat Frontend Environment Variables
# Copy this file to .env and fill in your values

# Backend API URL
VITE_API_URL=https://backend-op1c06n9l-everest-ans-projects.vercel.app

# WebSocket URL (usually same as API URL)
VITE_SOCKET_URL=https://backend-op1c06n9l-everest-ans-projects.vercel.app

# IPFS/Pinata Configuration (Optional - for file uploads)
# Get your keys from https://pinata.cloud
VITE_PINATA_API_KEY=
VITE_PINATA_SECRET_KEY=
VITE_PINATA_JWT=

# Stripe Configuration (Optional - for payments)
# Get your key from https://dashboard.stripe.com
VITE_STRIPE_PUBLIC_KEY=

# Infura Configuration (Optional - for Web3)
# Get your project ID from https://infura.io
VITE_INFURA_PROJECT_ID=

# WalletConnect Configuration (Optional)
VITE_WALLETCONNECT_PROJECT_ID=
